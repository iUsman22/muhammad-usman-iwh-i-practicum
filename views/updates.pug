doctype html
html
  head
    title= `${title}`
  body
    h1 Add a New Custom Object
    form(action="/update-cobj" method="POST")
      label(for="name") Name:
      input(type="text" name="name" required)
      br
      label(for="species") Species:
      input(type="text" name="species" required)
      br
      label(for="about") About:
      input(type="text" name="about" required)
      br
      button(type="submit") Submit
    br
    a(href="/") Return to homepage
